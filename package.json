{
  "name": "nuxt-app",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@10.20.0",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "typecheck": "nuxt typecheck",
    "ci": "pnpm lint && pnpm typecheck && pnpm build",
    "check:fix": "pnpm lint:fix && pnpm format",
    "db:generate": "drizzle-kit generate",
    "auth:schema": "pnpm dlx @better-auth/cli generate --config scripts/auth-cli.ts --output server/database/auth-schema.ts -y",
    "seed:talks": "curl -X POST http://localhost:3000/_nitro/tasks/seed-public-talks",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:report": "playwright show-report"
  },
  "dependencies": {
    "@nuxt/eslint": "1.10.0",
    "@nuxt/ui": "4.1.0",
    "@nuxthub/core": "0.9.0",
    "@nuxtjs/i18n": "10.1.2",
    "better-auth": "1.3.34",
    "drizzle-orm": "0.44.7",
    "eslint": "9.39.0",
    "nuxt": "4.2.0",
    "resend": "6.4.0",
    "typescript": "5.9.3",
    "vue": "3.5.22",
    "vue-router": "4.6.3",
    "zod": "4.1.12"
  },
  "devDependencies": {
    "@libsql/client": "0.15.15",
    "@playwright/test": "1.56.1",
    "@react-email/render": "1.4.0",
    "@types/node": "24.9.2",
    "adm-zip": "0.5.16",
    "drizzle-kit": "0.31.6",
    "prettier": "3.6.2",
    "tailwindcss": "4.1.16",
    "tinyexec": "1.0.1",
    "vitest": "4.0.6",
    "wrangler": "4.45.3"
  }
}
